{
    "name": "chatbot-app",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "client",
        "api"
    ],
    "scripts": {
        "start": "concurrently \"npm start --prefix client\" \"npm start --prefix api\"",
        "start:client": "npm start --prefix client",
        "start:api": "npm start --prefix api",
        "build": "npm run build --prefix client",
        "seed": "npm run seed --prefix api"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}
